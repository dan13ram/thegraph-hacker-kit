{
  "name": "@erc20-example/subgraph",
  "version": "1.0.0",
  "dependencies": {
    "@graphprotocol/graph-cli": "0.20.1",
    "@graphprotocol/graph-ts": "0.20.0"
  },
  "license": "MIT",
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/ $GRAPH_ACCESS_TOKEN",
    "build": "graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "create-local": "graph create --node http://localhost:8020/ workshop/subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ workshop/subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 workshop/subgraph"
  }
}
